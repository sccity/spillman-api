  "paths": {
    "/avl": {
      "get": {
        "tags": [
          "Unit"
        ],
        "summary": "AVL Data",
        "description": "Automatic vehicle location data",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "app",
            "in": "query",
            "description": "Application name/ID accessing the API",
            "required": false,
            "type": "string"
          },
          {
            "name": "uid",
            "in": "query",
            "description": "User name/ID accessing the API",
            "required": false,
            "type": "string"
          },
          {
            "name": "agency",
            "in": "query",
            "description": "Filter by agency code",
            "required": false,
            "type": "string"
          },
          {
            "name": "Unit",
            "in": "query",
            "description": "Filter by unit",
            "required": false,
            "type": "string"
          },
          {
            "name": "start",
            "in": "query",
            "description": "Filter results starting with this date in yyyy-mm-dd format",
            "required": false,
            "type": "string"
          },
          {
            "name": "end",
            "in": "query",
            "description": "Filter results ending with this date in yyyy-mm-dd format",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response"
          }
        }
      }
    },
