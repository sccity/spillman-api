            "required": false,
            "type": "string"
          },
          {
            "name": "page",
            "in": "query",
            "description": "Current page of results when data is paginated",
            "required": false,
            "type": "string"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "How many results to display, used with page in pagination",
            "required": false,
            "type": "string"
          }
        ],
        "security": [
          {
            "api_key": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response"
          }
        }
